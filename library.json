{
  "name": "pico-fcm-notifier",
  "version": "1.0.0",
  "description": "A library for securely provisioning WiFi and Firebase Cloud Messaging (FCM) credentials to Raspberry Pi Pico W boards over BLE (Bluetooth Low Energy), and for sending FCM notifications.",
  "keywords": "wifi, provision, ble, bluetooth, pico, raspberry pi pico, security, fcm, firebase cloud messaging, push notifications",
  "repository": {
    "type": "git",
    "url": "https://github.com/IoT-gamer/pico-fcm-notifier.git"
  },
  "authors": [
    {
      "name": "Sean P. Leary",
      "email": "sunrecorder1@gmail.com",
      "maintainer": true
    }
  ],
  "license": "MIT",
  "homepage": "https://github.com/ToT-gamer/pico-fcm-notifier",
  "dependencies": {
    "pico-ble-secure": "^1.2.0",
    "pico-ble-notify": "^1.0.1",
    "bblanchon/ArduinoJson": "^7.4.1"
  },
  "frameworks": "arduino",
  "platforms": "raspberrypi",
    "examples": [
      {
        "name": "BasicNotification",
        "base": "examples/BasicNotification",
        "files": [
          "src/main.cpp"
        ]
      }
    ],
    "export": {
        "exclude": [
          "examples/BasicNotification/.pio",
          "examples/BasicNotification/.vscode/.browse.c_cpp.db*",
          "examples/BasicNotification/.vscode/c_cpp_properties.json",
          "examples/BasicNotification/.vscode/launch.json",
          "examples/BasicNotification/.vscode/ipch",
          "examples/BasicNotification/logs/",
          ".git",
          ".github",
          "*.sh",
          "*.yml",
          "*.tar.gz"
        ]
      }
  }